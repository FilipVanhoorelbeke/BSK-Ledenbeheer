@model IEnumerable<BSKLedenManagement.Models.Kring>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Kring toevoegen", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Aanpassen", "Edit", new { id=item.Id }) 
            Verwijderen niet toegestaan.
        </td>
    </tr>
}

</table>
